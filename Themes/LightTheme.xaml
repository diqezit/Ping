<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BgPrimary" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="BgSecondary" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="BgHover" Color="#E5E5E5"/>
    <SolidColorBrush x:Key="BgInput" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="BgAccent" Color="#0078D4"/>
    <SolidColorBrush x:Key="BgAccentHover" Color="#1C97EA"/>
    <SolidColorBrush x:Key="BgAccentPressed" Color="#0062A3"/>
    <SolidColorBrush x:Key="BgDisabled" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="FgPrimary" Color="#1A1A1A"/>
    <SolidColorBrush x:Key="FgSecondary" Color="#555555"/>
    <SolidColorBrush x:Key="FgAccent" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="FgDisabled" Color="#A0A0A0"/>
    <SolidColorBrush x:Key="Border" Color="#D0D0D0"/>
    <SolidColorBrush x:Key="RouteLine" Color="#B0B0B0"/>
    <SolidColorBrush x:Key="RouteText" Color="#404040"/>
    <Color x:Key="RouteGoodColor">#0078D4</Color>
    <Color x:Key="RouteBadColor">#D41C1C</Color>
</ResourceDictionary>